import java.util.*;
class arraySum
 {
  public static void main(String[]args)
  {
   
   
   Scanner sc=new Scanner(System.in);
   System.out.print("Enter Array Size:");
   int n=sc.nextInt();
  
   int[]arr=new int[n];

   for(int i=0;i<n;i++)
	{
  System.out.print("Enter array element for::"+i+"----->");
	 arr[i]=sc.nextInt();
	}

   int sum=0;;
   
   for(int i=0;i<n;i++)
    {
         sum=sum+arr[i];       
    }

  System.out.print("SUM OF ELEMENTS IN THE ARRAY IS: "+sum);
}
}
